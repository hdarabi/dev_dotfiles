alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias gph='git push origin master'
alias gpl='git pull origin master'
alias rm='rm -iv'
alias cp='cp -iv'
alias mv='mv -iv'
alias mkdir='mkdir -p'
alias ls='ls -h --color'
alias ll='ls -alh --color=auto' 
alias agi='sudo apt-get install'
alias agr='sudo apt-get remove'
alias agu='sudo apt-get update'
